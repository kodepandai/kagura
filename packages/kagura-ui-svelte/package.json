{
	"name": "@kagura-ui/svelte",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit sync && svelte-package --watch",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"svelte": "dist/index.js",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"./dist/*"
	],
	"devDependencies": {
		"@floating-ui/dom": "^1.5.3",
		"@iconify/svelte": "^3.1.4",
		"@kagura-ui/preset-bootstrap": "workspace:^",
		"@kagura-ui/preset-mantine": "workspace:^",
		"@storybook/addon-docs": "7.4.6",
		"@storybook/addon-essentials": "^7.4.6",
		"@storybook/addon-interactions": "^7.4.6",
		"@storybook/addon-links": "^7.4.6",
		"@storybook/blocks": "^7.4.6",
		"@storybook/svelte": "^7.4.6",
		"@storybook/sveltekit": "^7.4.6",
		"@storybook/testing-library": "^0.2.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "1.25.1",
		"@sveltejs/package": "2.2.2",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"autoprefixer": "^10.4.16",
		"babel-loader": "^9.1.3",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-storybook": "^0.6.14",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "7.4.6",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-floating-ui": "^1.5.3",
		"svelte-loader": "^3.1.9",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.11"
	},
	"type": "module",
	"dependencies": {
		"@kagura-ui/core": "workspace:^"
	}
}
