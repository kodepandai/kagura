{
	"name": "@kagura-ui/svelte",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit sync && svelte-package --watch",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"svelte": "dist/index.js",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"./dist/*"
	],
	"devDependencies": {
		"@storybook/addon-docs": "^6.5.15",
		"@storybook/addon-essentials": "^7.0.0-beta.15",
		"@storybook/addon-interactions": "^7.0.0-beta.15",
		"@storybook/addon-links": "^7.0.0-beta.15",
		"@storybook/addon-svelte-csf": "^2.0.10",
		"@storybook/blocks": "^7.0.0-beta.15",
		"@storybook/svelte": "^7.0.0-beta.15",
		"@storybook/sveltekit": "^7.0.0-beta.15",
		"@storybook/testing-library": "^0.0.13",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@sveltejs/package": "1.0.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"babel-loader": "^8.3.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-storybook": "^0.6.8",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.0.0-beta.15",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.1",
		"svelte-loader": "^3.1.4",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4",
		"vite": "^4.0.3"
	},
	"type": "module"
}
